{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "ovbooking",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "API_KEY",
      "expirationTime": 7
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "OPTIMISTIC_CONCURRENCY"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "OPTIMISTIC_CONCURRENCY"
          },
          "entityName": "Reservation"
        }
      ]
    }
  }
}